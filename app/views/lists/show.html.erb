<h2><%= @list.name %></h2>
<%= link_to "Edit",
  edit_list_path
%>
<br>
<p><strong>Bookmarks</strong></p>
<ul class="list-group">
  <% @bookmarks.each do |bookmark| %>
    <li class="list-group-item"><%= bookmark.comment %></li>
  <% end %>
</ul>
<%= link_to "Write new bookmark here",  new_list_bookmark_path(@list) %>
